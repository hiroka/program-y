    <category>
        <pattern>GET ABC</pattern>
        <template>
            <extension path="programy.extensions.newsapi.newsapi.NewsAPIExtension">
                SOURCE abc-news-au MAX 10 SORT False REVERSE False
            </extension>
        </template>
    </category>

    <category>
        <pattern>*</pattern>
        <template>
            <extension path="programy.extensions.talkapi.talkapi.TalkAPIExtension">
				<star index="1"/>
            </extension>
        </template>
    </category>

    <category>
        <pattern>
           NEWS * MAX * SORT * REVERSE *
        </pattern>
        <template>
            <extension path="programy.extensions.newsapi.newsapi.NewsAPIExtension">
                SOURCE <star index="1"/> MAX <star index="2"/> SORT <star index="3"/> REVERSE <star index="4"/>
            </extension>
        </template>
    </category>

    <category>
