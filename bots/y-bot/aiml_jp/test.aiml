    <category>
      <pattern>どこ</pattern>
      <template>
        <think>
          <set name="place"></set>
          <set name="who"></set>
        </think>
        どこへいきましょう
      </template>
    </category>

    <category>
      <pattern>*</pattern>
	     <that>どこへいきましょう</that>
	      <template>
          <think><set name="place"><star index="1"/></set></think>
          誰と行きますか？
        </template>
    </category>

    <category>
      <pattern>*</pattern>
      <that>誰と行きますか？</that>
      <template>
        <think><set name="who"><star index="1"/></set></think>
        <get name="place"/>に<get name="who"/>と行くんですね。いいなー
      </template>
    </category>

